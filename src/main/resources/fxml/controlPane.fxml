<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.layout.HBox?>

<HBox alignment="CENTER_LEFT" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.javastart.mp3player.controller.ControlPaneController">
    <Button id="prev" fx:id="previousButton" mnemonicParsing="false" prefHeight="30.0" prefWidth="30.0" HBox.hgrow="NEVER" />
    <ToggleButton id="play" fx:id="playButton" mnemonicParsing="false" prefHeight="30.0" prefWidth="30.0" HBox.hgrow="NEVER" />
    <Button id="next" fx:id="nextButton" mnemonicParsing="false" prefHeight="30.0" prefWidth="30.0" HBox.hgrow="NEVER" />
    <Slider fx:id="volumeSlider" HBox.hgrow="NEVER">
      <padding>
         <Insets left="10.0" right="10.0" />
      </padding></Slider>
    <Slider fx:id="progressSlider" HBox.hgrow="ALWAYS">
      <padding>
         <Insets left="10.0" right="10.0" />
      </padding></Slider>
</HBox>
